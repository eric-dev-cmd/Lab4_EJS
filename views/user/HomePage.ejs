<div class="container">
  <div class="row row-cols-1 row-cols-md-3 g-4 my-3">
    <% subjects.forEach(function(subject){ %>
    <div class="col">
      <div class="card">
        <div class="card-body">
          <h5 class="card-title"><%= subject.name%></h5>
          <p class="card-text"><%= subject.subjectType%></p>
          <p class="card-text"><%= subject.department%></p>
        </div>
      </div>
    </div>
    <%});%>
  </div>
</div>
